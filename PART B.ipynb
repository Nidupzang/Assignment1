   "source": [
    "\n",
    "def find_outliers(variable, factor= 3, print_summary=True):\n",
    "    # Inter-quartile range = Q3 - Q1\n",
    "    IQR = df_filtered[variable].quantile(0.75) - df_filtered[variable].quantile(0.25)\n",
    "    lower_boundary = df_filtered[variable].quantile(0.25) - (IQR * factor)\n",
    "    upper_boundary = df_filtered[variable].quantile(0.75) + (IQR * factor)\n",
    "    \n",
    "    # list to store the outliers\n",
    "    outliers= []\n",
    "    for index, val in enumerate(df_filtered[variable]):\n",
    "        if val < lower_boundary or val > upper_boundary:\n",
    "            outliers.append(index)\n",
    "    \n",
    "    \n",
    "    if(print_summary):\n",
    "        print(f'{variable} outliers are values < {lower_boundary} or > {upper_boundary}')\n",
    "        print(f\"There are {len(outliers)} outliers\")\n",
    "    #return outliers"
   ]
