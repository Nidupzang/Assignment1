    "import numpy as np\n",
    "\n",
    "# Function to replace outlier with maximum value\n",
    "def cap_max_value(df_temp, variable, top):\n",
    "    df_temp[variable] = np.where(df_temp[variable] > top, top, df_temp[variable])\n",
    "\n",
    "# Function to replace outlier with minimum value\n",
    "def cap_min_value(df_temp, variable, lower):\n",
    "    df_temp[variable] = np.where(df_temp[variable] < lower, lower, df_temp[variable])\n",
    "\n",
    "# Define thresholds for outliers\n",
    "variable_with_upper_outliers = {\n",
    "    'cost': 96.0,\n",
    "    'votes': 215.0,\n",
